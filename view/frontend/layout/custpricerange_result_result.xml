<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
   <update handle="customer_account" />

   <head>
      <script src="Msquared_CustPriceRange::js/cpr.js" />
   </head>
  
   <body>
      <referenceBlock name="page.main.title">
         <action method="setPageTitle">
            <argument translate="true" name="title" xsi:type="string">Custom Price Range Results</argument>
         </action>
      </referenceBlock>
      <referenceContainer name="content">
         <block class="Msquared\CustPriceRange\Block\Index" name="range_result" template="Msquared_CustPriceRange::result.phtml">
         </block>
      </referenceContainer>
   </body>
</page>
